  /*faq start*/
  .faq_sec
  {
    padding: 30px 20px;
    background: #eaeaea;
    border-radius: 10px;
    position: relative;
  }
  .faq_sec h1
  {
    font-family: 'Lato', sans-serif;
    font-size: 30px;
    line-height: 1.2;
    font-weight: 700;
    color: #181818;
    text-align: center;
    margin: 0 0 20px;
    text-transform: uppercase;
  }
  .faq-body
  {
    /* box-shadow: 0px 4px 10px 0px rgba(0, 0, 0, 0.10); */
    border-radius: 0.2em;
    /* padding: 1em; */
    /* background-color: #fff; */
    height: 263px;
    overflow: auto;
  }
  .faq-list {
    width: 95%;
    margin: 0 auto;
    padding: 0;
  }
  .faq-list > div {
    border-bottom: 0.07em solid #ededed;
    padding: 5px 0em;
  }
  details {
    padding: 10px;
    transition: all 0.5s ease-in-out;
  }
  details[open] {
    box-shadow: 0px 5px 10px rgba(0, 0, 0, 0.3);
  }
  summary {
    font-size: 16px;
    font-weight: bold;
    cursor: pointer;
    outline: none;
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
  }
  details[open] summary {
    color: #DA2700;
  }
  summary:after {
    background: transparent;
    border-radius: 0.3em;
    content: "+";
    color: #DA2700;
    float: left;
    font-size: 1.8em;
    font-weight: bold;
    margin: -0.3em 0.25em 0 0;
    padding: 0;
    text-align: center;
    width: 25px;
  }
  details[open] summary:after {
    content: "-";
    font-size: 3.2em;
    margin: -0.5em 0.14em 0 0;
    font-weight: 200;
  }
  details[open] p {
    border-left: 2px solid #DA2700;
    margin-left: 37px;
    padding-left: 25px;
    opacity: 100;
    font-size: 14px;
    transition: all 3s ease;
  }
  details[open] summary ~ * {
    animation: sweep .5s ease-in-out;
  }
  
  
.gray {
  color: #a5a5a5;
