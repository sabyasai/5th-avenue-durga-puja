@font-face {
    font-family: 'golden_hills_demoregular';
    src: url('../fonts/goldenhillsdemo-webfont.woff2') format('woff2'),
         url('../fonts/goldenhillsdemo-webfont.woff') format('woff');
    font-weight: normal;
    font-style: normal;

}
body
{
	margin: 0;
	padding: 0;
	font-family: 'Montserrat', sans-serif;
  overflow-x: hidden;
}
/*header start*/
header
{
	background: #f2f2f2;
}
header ul.top_contact
{
	margin: 0;
	padding: 0;
	list-style: none;
	background: #fff;
	display: inline-block;
}
header ul.top_contact li
{
	font-family: 'Lato', sans-serif;
	display: inline-block;
	font-size: 14px;
	line-height: 1.5;
	font-weight: 400;
	color: #7c0000;
	padding: 8px;
}
header ul.top_contact li img
{
	margin-right: 5px;
}
.social_media
{
	margin: 0;
	padding: 3px 0;
	list-style: none;
	display: flex;
	flex-wrap: wrap;
	justify-content: flex-end;
}
.social_media li
{
	margin: 2px;
}
.social_media li img
{
	width: 20px;
}
/*header end*/
/*banner start*/
.banner
{
	position: relative;
  z-index: 0;
}
.banner:before
{
	content: '';
	position: absolute;
	left: 0;
	bottom: -19px;
	width: 100%;
	height: 20px;
	background: url(../images/divider.png);
	background-position: center;
	background-size: contain;
	z-index: 99;
}
.logo {
    position: absolute;
    right: 8%;
    top: 22px;
    width: 220px;
    z-index: 999;
}
.banner .carousel-caption
{
	right: 10%;
	bottom: -50px;
	left: inherit;
	opacity: 0;
	transition: all .5s ease-in-out;
}
.banner .carousel-item.active .carousel-caption
{
	bottom: 50px;
	opacity: 1;
}
.carousel-item
{
  height: 500px;
}
.carousel-item img 
{
  width: 100%;
  height: 100%;
  object-fit: cover;
}
.banner .carousel-caption h3
{
	font-family: 'golden_hills_demoregular';
	font-size: 63px;
	line-height: 1;
	color: #be0404;
}
.banner .carousel-caption h3 span
{
	display: block;
	font-size: 120px;
}
.carousel-indicators
{
	bottom: 31px;
}
.carousel-indicators .active
{
	background-color: #be0404;
}
/*banner end*/
/*banner bottom start*/
.banner_bottom
{
	min-height: 150px;
	padding: 50px 0;
}
.banner_bottom h3
{
	font-family: 'Lato', sans-serif;
	font-size: 20px;
	line-height: 1.2;
	font-weight: 600;
	color: #000000;
}
.scrollbar
{
	height: 300px;
	overflow-y: scroll;
}
.force-overflow
{
	min-height: 150px;
}
.scrollbar::-webkit-scrollbar-track
{
	-webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3);
	background-color: #F5F5F5;
}

.scrollbar::-webkit-scrollbar
{
	width: 3px;
	background-color: #dedede;
}

.scrollbar::-webkit-scrollbar-thumb
{
	background-color: #ffc600;
	border: 2px solid #ffc600;
	border-radius: 10px;
}
.current_committee
{
	padding: 4px 10px;
}
.current_committee ul
{
	list-style: none;
	margin: 0;
	padding: 0;
}
.current_committee ul li
{
	display: flex;
	flex-wrap: wrap;
	margin: 0 0 15px;
}
.current_committee ul li .current_committee_img
{
	flex: 0 0 70px;
	height: 70px;
	border-radius: 50%;
	overflow: hidden;
}
.current_committee ul li .current_committee_text
{
	flex: 0 0 calc(100% - 70px);
	padding: 0 8px;
}
.current_committee_text h4,
.puja_schedule_details h4,
.announcements_ul li a .announcements_text h4
{
	font-family: 'Lato', sans-serif;
	font-size: 18px;
	line-height: 1.2;
	font-weight: 600;
	color: #ff9400;
	margin: 0 0 5px;
}
.current_committee_text h4 span
{
	display: block;
	font-size: 12px;
	color: #929090;
}
.current_committee_text p
{
	font-size: 14px;
	line-height: 1.2;
	color: #000;
	font-weight: 600;
}
/*banner bottom end*/

/*timer start*/
.puja_timer
{
  padding-top: 3rem;
}
.puja_timer_ul
{
	margin: 0;
	padding: 0;
	list-style: none;
	display: flex;
	flex-wrap: wrap;
	justify-content: center;
	text-align: center;
}
.puja_timer_ul li {
  flex: 0 0 50px;
  padding: 10px;
  font-size: 20px;
  font-weight: 600;
  color: #181818;
  text-transform: capitalize;
  position: relative;
}

.puja_timer_ul li span {
  display: block;
  font-size: 70px;
  line-height: 1.2;
  color: #EC2B00;
  font-weight: 700;
}
.puja_timer_ul li:first-child:before,
.puja_timer_ul li:nth-child(2):before,
.puja_timer_ul li:nth-child(3):before
{
	content: "";
	width: 1px;
	height: 40%;
	position: absolute;
	top: 50%;
	right: 0;
	transform: translateY(-50%);
	background: #181818;
}
/*timer end*/
.puja_schedule ul
{
	margin: 0;
	padding: 0;
	list-style: none;
}
.puja_schedule li
{
	display: flex;
	flex-wrap: wrap;
	justify-content: center;
	align-items: center;
	margin: 0 0 15px;
}
.puja_schedule li .puja_schedule_date
{
	flex: 0 0 90px;
	max-width: 90px;
	height: 90px;
	border-radius: 50%;
	background: #ffc600;
	display: flex;
    justify-content: center;
    align-items: center;
    text-align: center;
}
.puja_schedule li .puja_schedule_details
{
	flex: 0 0 calc(100% - 90px);
	padding: 0 0 0 10px;
}
.puja_schedule_date p
{
	font-size: 20px;
	line-height: 1.2;
	text-transform: uppercase;
	color: #000;
	font-weight: 600;
	margin: 0;
	padding: 0;
}
.puja_schedule_date p span
{
	display: block;
	font-size: 14px;
}
.puja_schedule_details p
{
	font-size: 14px;
	line-height: 1.2;
	font-weight: 600;
	margin: 0;
    color: #929090;
}
.puja_schedule_details .btn_more
{
	font-size: 14px;
	line-height: 1.5;
	color: #000;
	display: block;
}
.modal-dialog {
    max-width: 75%;
}
.modal-content {
    border: none;
    border-radius: 0;
}
.modal-body {
    padding: 40px 15px 35px;
}
.modal-content .close {
    padding: 0;
    border: 2px solid #000;
    width: 30px;
    height: 30px;
    border-radius: 50%;
    margin: 0 0 0 auto;
}
.modal-title span
{
	display: block;
	font-size: 14px;
}
.announcements_ul
{
	margin: 0;
	padding: 0;
	list-style: none;
}
.announcements_ul li
{
	margin: 0 0 15px;
}
.announcements_ul li a
{
	display: flex;
	flex-wrap: wrap;
	justify-content: center;
	align-items: center;
	text-decoration: none;
}
.announcements_ul li a .announcements_icon
{
	flex: 0 0 80px;
	max-width: 80px;
	height: 80px;
	border-radius: 50%;
	overflow: hidden;
}
.announcements_ul li a .announcements_text
{
	flex: 0 0 calc(100% - 80px);
	padding-left: 10px;
}

 /*gallery start*/
 .gallery
 {
 	padding: 60px 0;
 }
 .gallery .item
 {
 	position: relative;
 	transition: all .5s ease-in-out;
 }
 .gallery .item .gallery_img 
 {
  height: 200px;
 	margin: 10px 0;
 	transition: all .5s ease-in-out;
 }
  .gallery .item .gallery_img  img 
  {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }
 .gallery .item:hover .gallery_img
 {
 	margin: 0;
 	border:3px solid #ffc600;
 	position: relative;
 	transform: scale(1);
 	z-index: 9999;
 }
 .gallery .item:hover .gallery_img:before
 {
 	content: "";
 	position: absolute;
 	top: 0;
 	left: 0;
 	width: 100%;
 	height: 100%;
 	background: rgba(0,0,0,.6);
 }
 .plus
 {
 	position: absolute;
 	top: 50%;
 	left: 50%;
 	width: 50px;
 	height: 50px;
 	border-radius: 50%;
 	background: #F6C305;
 	font-size: 25px;
 	line-height: 1.2;
 	text-align: center;
 	z-index: 999;
 	display: flex;
    justify-content: center;
    align-items: center;
    transform: translate(-50% ,-50%);
    opacity: 0;
    transition: all .5s ease-in-out;
 }
  .gallery .item:hover .plus
  {
  	opacity: 1;
  }
  .owl-gallery .owl-nav .owl-prev, .owl-gallery .owl-nav .owl-next
  {
  	font-size: 40px !important;
    position: absolute;
    top: 50%;
    transform: translateY(-85%);
    transition: all .5s ease-in-out;
  }
  .owl-gallery .owl-nav .owl-prev {
    left: -30px;
   }
   .owl-gallery .owl-nav .owl-next {
    right: -30px;
	}
	.owl-gallery .owl-nav .owl-prev:focus, .owl-gallery .owl-nav .owl-next:focus
	{
		box-shadow: none;
		outline: none;
	}
 /*gallery end*/
